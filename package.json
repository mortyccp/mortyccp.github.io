{
  "name": "mortyccp.github.io",
  "homepage": "https://mortyccp.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -b master -d build",
    "test": "react-scripts test --env=jsdom",
