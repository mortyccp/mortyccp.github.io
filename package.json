  "dependencies": {
    "@svgr/cli": "^2.3.0",
    "classnames": "^2.2.6",
    "gh-pages": "^1.2.0",
    "normalize.css": "^8.0.0",
    "prettier": "^1.14.2",
    "puppeteer": "^1.8.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "singleQuote": true
  }
}
